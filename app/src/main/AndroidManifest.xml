<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="iot.polytech.banduke">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activities.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.SignUpActivity" />
        <activity android:name=".activities.LogInActivity" />
        <activity android:name=".activities.ProfileActivity" />
        <activity android:name=".activities.FriendsActivity" />
        <activity android:name=".activities.EditProfileActivity" />
        <activity android:name=".activities.SearchActivity" />
        <activity android:name=".activities.ListSessionActivity" />
        <activity android:name=".activities.SessionActivity" />
        <activity android:name=".activities.EditSessionActivity" />
        <activity android:name=".activities.GyrDataActivity" />
        <activity android:name=".activities.AccDataActivity" />
        <activity android:name=".activities.GpsDataActivity" />
        <activity android:name=".activities.AddSessionActivity" />
        <activity android:name=".activities.AltSpeedDataActivity"></activity>
    </application>

</manifest>